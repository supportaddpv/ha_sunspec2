[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = SAN
prompt             = no

[ req_distinguished_name ]
C  = US
ST = STATE
L  = LOCAL
O  = ORG
OU = SUBORG
CN = SunSpecModbusSecurityServer

[ SAN ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
IP.1  = 127.0.0.1